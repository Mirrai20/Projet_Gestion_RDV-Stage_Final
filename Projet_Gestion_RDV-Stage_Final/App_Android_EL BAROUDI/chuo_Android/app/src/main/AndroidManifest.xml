<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.chuo_android">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logovertical"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logovertical"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".Historique_AllRdvPatient">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.chuo_android.Accueil" />
        </activity>
        <activity android:name=".rdvAllPatient">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.chuo_android.Accueil" />
        </activity>
        <activity android:name=".Consulter_RDV">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.chuo_android.Accueil" />
        </activity>
        <activity android:name=".Prendre_RDV">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.chuo_android.specialite" />
        </activity>
        <activity android:name=".suiverRecl">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.chuo_android.Accueil" />
        </activity>
        <activity android:name=".Reclamation">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.chuo_android.Accueil" />
        </activity>
        <activity android:name=".historique">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.chuo_android.Accueil" />
        </activity>
        <activity
            android:name=".specialite"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.chuo_android.Accueil" />
        </activity>
        <activity
            android:name=".Accueil"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".ProfileActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.chuo_android.Accueil" />
        </activity>
        <activity android:name=".LoginActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>